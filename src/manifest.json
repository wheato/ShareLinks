{
  "name": "ShareLinks",
  "version": "0.1.0",
  "description": "Copy website url and title at the same time.",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["backgroundContextMenus.js"]
  },
  "permissions": ["activeTab", "contextMenus", "clipboardWrite"]
}